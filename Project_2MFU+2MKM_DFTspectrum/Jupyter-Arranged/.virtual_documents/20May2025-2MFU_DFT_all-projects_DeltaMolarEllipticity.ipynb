plt.xticks(np.arange(200, 352, 10), fontsize=16)
plt.yticks(fontsize=16)
ax.set_xlim(200, 352)
plt.xlim(200, 352)
plt.legend(loc='upper right', fontsize=16, frameon=False, bbox_to_anchor=(0.48, 1.02))
