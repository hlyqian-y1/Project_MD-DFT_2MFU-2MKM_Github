axes[2].axvspan(240, 255, facecolor='#ffcc99', alpha=0.6, zorder=0, linewidth=1.0)
axes[2].axvspan(290, 300, facecolor='#99ccff', alpha=0.6, zorder=0, linewidth=1.0)

